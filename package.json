{
  "name": "syntax-sage-server",
  "description": "",
  "version": "0.1.2",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "krislintigo"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 18.14.2"
  },
  "feathers": {
    "language": "ts",
    "packager": "pnpm",
    "database": "mongodb",
    "framework": "koa",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js"
  ],
  "main": "lib/client",
  "scripts": {
    "dev": "nodemon -x ts-node src/index.ts",
    "compile": "shx rm -rf lib/ && tsc --skipLibCheck",
    "start": "node lib/",
    "prettier": "npx prettier \"**/*.ts\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "test": "npm run mocha",
    "bundle:client": "npm run compile && npm pack --pack-destination ./public"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.14",
    "@feathersjs/authentication-client": "^5.0.14",
    "@feathersjs/authentication-local": "^5.0.14",
    "@feathersjs/configuration": "^5.0.14",
    "@feathersjs/errors": "^5.0.14",
    "@feathersjs/feathers": "^5.0.14",
    "@feathersjs/koa": "^5.0.14",
    "@feathersjs/mongodb": "^5.0.14",
    "@feathersjs/schema": "^5.0.14",
    "@feathersjs/socketio": "^5.0.14",
    "@feathersjs/transport-commons": "^5.0.14",
    "@feathersjs/typebox": "^5.0.14",
    "lodash": "^4.17.21",
    "mongodb": "^6.3.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@feathersjs/authentication": "^5.0.14",
    "@feathersjs/cli": "5.0.14",
    "@feathersjs/rest-client": "^5.0.14",
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.0",
    "axios": "^1.6.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "migrate-mongo": "^11.0.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
